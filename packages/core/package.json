{
  "name": "@yetzirah/core",
  "version": "0.1.0",
  "description": "Web Components for behavioral UI patterns - zero dependencies, framework-agnostic",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cdn": {
      "import": "./dist/yetzirah.js"
    },
    "./button": {
      "types": "./dist/button.d.ts",
      "import": "./dist/button.js",
      "require": "./dist/button.cjs"
    },
    "./disclosure": {
      "types": "./dist/disclosure.d.ts",
      "import": "./dist/disclosure.js",
      "require": "./dist/disclosure.cjs"
    },
    "./dialog": {
      "types": "./dist/dialog.d.ts",
      "import": "./dist/dialog.js",
      "require": "./dist/dialog.cjs"
    },
    "./tabs": {
      "types": "./dist/tabs.d.ts",
      "import": "./dist/tabs.js",
      "require": "./dist/tabs.cjs"
    },
    "./tooltip": {
      "types": "./dist/tooltip.d.ts",
      "import": "./dist/tooltip.js",
      "require": "./dist/tooltip.cjs"
    },
    "./menu": {
      "types": "./dist/menu.d.ts",
      "import": "./dist/menu.js",
      "require": "./dist/menu.cjs"
    },
    "./autocomplete": {
      "types": "./dist/autocomplete.d.ts",
      "import": "./dist/autocomplete.js",
      "require": "./dist/autocomplete.cjs"
    },
    "./listbox": {
      "types": "./dist/listbox.d.ts",
      "import": "./dist/listbox.js",
      "require": "./dist/listbox.cjs"
    },
    "./select": {
      "types": "./dist/select.d.ts",
      "import": "./dist/select.js",
      "require": "./dist/select.cjs"
    },
    "./accordion": {
      "types": "./dist/accordion.d.ts",
      "import": "./dist/accordion.js",
      "require": "./dist/accordion.cjs"
    },
    "./drawer": {
      "types": "./dist/drawer.d.ts",
      "import": "./dist/drawer.js",
      "require": "./dist/drawer.cjs"
    },
    "./popover": {
      "types": "./dist/popover.d.ts",
      "import": "./dist/popover.js",
      "require": "./dist/popover.cjs"
    },
    "./toggle": {
      "types": "./dist/toggle.d.ts",
      "import": "./dist/toggle.js",
      "require": "./dist/toggle.cjs"
    },
    "./chip": {
      "types": "./dist/chip.d.ts",
      "import": "./dist/chip.js",
      "require": "./dist/chip.cjs"
    },
    "./icon-button": {
      "types": "./dist/icon-button.d.ts",
      "import": "./dist/icon-button.js",
      "require": "./dist/icon-button.cjs"
    },
    "./slider": {
      "types": "./dist/slider.d.ts",
      "import": "./dist/slider.js",
      "require": "./dist/slider.cjs"
    },
    "./datagrid": {
      "types": "./dist/datagrid.d.ts",
      "import": "./dist/datagrid.js",
      "require": "./dist/datagrid.cjs"
    },
    "./dark.css": "./src/dark.css",
    "./button.css": "./button.css",
    "./disclosure.css": "./disclosure.css",
    "./dialog.css": "./dialog.css"
  },
  "files": [
    "dist",
    "src/dark.css",
    "*.css"
  ],
  "scripts": {
    "build": "tsup",
    "build:cdn": "tsup --config tsup.cdn.config.js",
    "dev": "tsup --watch"
  },
  "keywords": [
    "web-components",
    "ui",
    "accessibility",
    "a11y",
    "material",
    "headless"
  ],
  "author": "Grimoire Intelligence",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grimoire-intelligence/yetzirah.git",
    "directory": "packages/core"
  },
  "sideEffects": false
}
